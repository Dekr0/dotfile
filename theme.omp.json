{
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#00aedb",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "<#00aedb>\uf0e7</>  <b>{{ .Path }}</b>  ",
          "type": "path"
        },
        {
          "foreground": "#ffc425",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "diamond",
          "template": "<#ffc425>\u26A1</> <b>{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}</b>  ",
          "type": "git"
        },
        {
          "foreground": "#f37735",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": "{{ .FormattedMs }}s  ",
          "type": "executiontime"
        },
        {
          "foreground": "#d11141",
          "style": "diamond",
          "template": "\uf071  {{ .Meaning }}",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#c0c0c0",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#d11141{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\uf29b ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
